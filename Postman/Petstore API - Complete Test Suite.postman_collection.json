				{
					"name": "TC-PET-002: Get Pet by ID",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// 1. Pastikan Request Berhasil (Status 200)\r",
									"pm.test(\"Status code is 200 OK\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"// 2. Pastikan Response adalah JSON yang valid\r",
									"pm.test(\"Response is a valid JSON\", function () {\r",
									"    pm.response.to.be.withBody;\r",
									"    pm.response.to.be.json;\r",
									"});\r",
									"\r",
									"// 3. Validasi Konten Data\r",
									"pm.test(\"Pet details match expected values\", function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    \r",
									"    const rawResponse = pm.response.text();\r",
									"    const idMatch = rawResponse.match(/\"id\":\\s*(\\d+)/);\r",
									"    const actualId = idMatch ? idMatch[1] : null;\r",
									"\r",
									"    // Ambil ID dari environment\r",
									"    const expectedId = pm.environment.get(\"pet_id\");\r",
									"    \r",
									"    // Bandingkan ID sebagai string murni\r",
									"    pm.expect(actualId).to.eql(expectedId);\r",
									"    \r",
									"    // Assert Properti Utama\r",
									"    pm.expect(jsonData).to.have.property('name');\r",
									"    pm.expect(jsonData).to.have.property('status');\r",
									"});\r",
									"\r",
									"// 4. Verifikasi Tipe Data Khusus (Opsional)\r",
									"pm.test(\"Data types are correct\", function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    pm.expect(jsonData.id).to.be.a('number');\r",
									"    pm.expect(jsonData.name).to.be.a('string');\r",
									"});"
								],
								"type": "text/javascript",
								"packages": {},
								"requests": {}
							}
						}
					],
