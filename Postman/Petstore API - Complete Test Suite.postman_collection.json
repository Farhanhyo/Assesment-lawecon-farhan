									"});\r",
									"\r",
									"// 2. Ambil ID secara aman (menghindari pembulatan angka besar)\r",
									"const rawResponse = pm.response.text();\r",
									"const idMatch = rawResponse.match(/\"id\":\\s*(\\d+)/);\r",
									"const actualId = idMatch ? idMatch[1] : null;\r",
									"const expectedId = pm.environment.get(\"pet_id\");\r",
									"\r",
									"// 3. Validasi Konten Data yang Diperbarui\r",
									"pm.test(\"Pet data updated successfully\", function () {\r",
									"    const jsonData = pm.response.json();\r",
									"    \r",
									"    // Verifikasi ID tetap sama dengan yang dikirim\r",
									"    pm.expect(actualId).to.eql(expectedId);\r",
									"    \r",
									"    // Verifikasi Nama baru (Pastikan sesuai dengan yang Anda tulis di Body PUT)\r",
									"    // Misalnya Anda mengubah nama menjadi \"Buddy Updated\"\r",
									"    pm.expect(jsonData.name).to.eql(\"Buddy Updated\");\r",
									"    \r",
									"    // Verifikasi Status baru (Misalnya dari 'available' menjadi 'sold')\r",
